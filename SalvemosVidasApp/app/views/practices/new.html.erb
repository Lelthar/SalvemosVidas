<% if signed_in? %>
	<div class="container" style="background: #fafafa; width: 95%; height: 90%; padding-bottom: 20px;">
		<h2 align="center">Creación de Practica</h2>

		<%= render 'form', practice: @practice %>

		<div class="col-sm-offset-2 col-sm-10">
	        <%= button_to 'Volver', practices_path, class:"btn" , method: :get %>
	    </div>
	</div>
	

<% else %>
    <div class="alert alert-danger panel-ancho">
      <h4>Error!</h4>
      <p>Necesita iniciar sesión para acceder a esta dirección.</p>
    </div>
<% end %>
